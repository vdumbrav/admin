{
  "name": "waitlist-admin",
  "private": true,
  "version": "1.4.2",
  "license": "UNLICENSED",
  "description": "Internal admin UI for managing the Waitlist service with OIDC authentication and MCP integration",
  "type": "module",
  "scripts": {
    "dev": "npm run api:ensure && vite",
    "build": "npm run api:ensure && tsc -b && vite build",
    "lint": "eslint .",
    "lint:ci": "eslint . --config eslint.ci.config.js",
    "preview": "vite preview",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "knip": "knip",
    "typecheck": "npm run generate:api:full && tsc --build --force --noEmit",
    "typecheck:strict": "npm run generate:api:full && tsc --noEmit --project tsconfig.app.json",
    "typecheck:strict:fast": "tsc --noEmit --project tsconfig.app.json",
    "typecheck:fast": "tsc --build --force --noEmit",
    "generate:api": "orval --config orval.config.ts",
    "update:api:barrel": "tsx src/lib/api/scripts/update-api-barrel.ts",
    "api:update": "tsx src/lib/api/scripts/update-api.ts",
    "api:ensure": "tsx src/lib/api/scripts/ensure-api.ts",
    "check": "npm run lint -- --fix --quiet && npm run typecheck:fast && npm run format",
    "generate:api:full": "npm run generate:api && npm run update:api:barrel",
    "check:full": "npm run generate:api:full && npm run lint -- --fix --quiet && npm run typecheck:strict:fast && npm run format",
    "postinstall": "npm run api:ensure"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/themes": "^3.2.1",
    "@tabler/icons-react": "^3.35.0",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.132.23",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.544.0",
    "oidc-client-ts": "^3.3.0",
    "react": "^19.1.1",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.63.0",
    "react-number-format": "^5.4.4",
    "react-oidc-context": "^3.3.0",
    "react-top-loading-bar": "^3.0.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router-devtools": "^1.132.23",
    "@tanstack/router-plugin": "^1.132.23",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "knip": "^5.64.1",
    "orval": "^7.13.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "shadcn": "^3.3.1",
    "tsx": "^4.20.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7"
  }
}
